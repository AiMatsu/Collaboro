<div class="container">
	<div class="row">
		<div class="col-md-6 col-md-offset-3 form-group">
			<h3><b>コラボリクエスト・編集</b></h3>
			<%= form_for(@request) do |f| %>

			<%= render 'requests/request_post',f:f %>

			<p class="actions">
				<%= f.submit "更新する" ,class: "btn btn-block btn-default btn-xs"%>
				<%= link_to '戻る', :back ,class: "btn btn-block btn-default btn-xs"%>
			</p>

			<% end %>
		</div>
	</div>
</div>