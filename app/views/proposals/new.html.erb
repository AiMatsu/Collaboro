<div class="container">
	<div class="row">
		<div class="col-md-6 col-md-offset-3 form-group">
			<h3><b>プロポーザル・新規投稿</b></h3>
			<%= form_for(@proposal) do |f| %>
				<%= render 'proposals/proposal_post',f:f %>
				<p class="actions">
					<%= f.submit "投稿する" ,class: "btn btn-block btn-default btn-xs"%>
					<%= link_to '戻る', :back ,class: "btn btn-block btn-default btn-xs"%>
				</p>
			<% end %>
		</div>
	</div>
</div>